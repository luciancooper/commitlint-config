{
  "name": "@lcooper/commitlint-config",
  "version": "1.1.0",
  "description": "Shareable commitlint config for enforcing conventional commits",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/commitlint-config.git"
  },
  "keywords": [
    "commitlint",
    "commitlint-config"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luciancooper/commitlint-config/issues"
  },
  "homepage": "https://github.com/luciancooper/commitlint-config#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "./index.js"
    ]
  },
  "dependencies": {
    "@commitlint/config-conventional": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^10.0.0",
    "husky": "^3.1.0"
  },
  "peerDependencies": {
    "@commitlint/cli": ">= 8"
  }
}
