{
  "name": "@lcooper/commitlint-config",
  "version": "1.1.0",
  "description": "Shareable commitlint config for enforcing conventional commits",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/commitlint-config.git"
  },
  "keywords": [
    "commitlint",
    "commitlint-config"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luciancooper/commitlint-config/issues"
  },
  "homepage": "https://github.com/luciancooper/commitlint-config#readme",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "commitlint": {
    "extends": [
      "./index.js"
    ]
  },
  "dependencies": {
    "@commitlint/config-conventional": "^19.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/lint": "^19.1.0",
    "@commitlint/load": "^19.2.0",
    "@lcooper/eslint-config": "^3.0.0",
    "@lcooper/eslint-config-jest": "^2.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=19"
  }
}
